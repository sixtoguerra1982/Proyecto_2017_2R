<%= form_with(model: [@cook, @comment]) do |form| %>

  <div class="form-group">
    <%= form.label :content, "Escribe tu comentario:", class: 'font-2' %>
    <div id="rating-form"></div>
    <%= form.text_area :content, id: :comment_content, class: 'form-control' %>
  </div>

  <div class="actions">
    <%= form.submit "comentar", class: 'btn btn-outline-primary font-1' %>
  </div>
<% end %>
